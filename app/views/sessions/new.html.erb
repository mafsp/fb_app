<body>
  <h1>Log in</h1>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <div class="log_in">
      <%= f.label :email %>
      <%= f.email_field :email %>
      <br>
      <br>
      <br>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <%= f.submit "Log in" %>
  <% end %>
</body>
